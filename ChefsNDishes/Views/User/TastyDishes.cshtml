@model List<Dish>

<h1 class="text-center"</h1> 
<table class="table table-striped">
<thead>
    <tr>
        <th>Name</th>
        <th>Chef</th>
        <th>Tastiness</th>
        <th>Calories</th>
    </tr>
    </thead>
    <tbody>
        @foreach (Dish d in Model)
        {
            <tr>
                <th>@d.DishName</th>
                <th>@d.Creator.FirstName</th>
                <th>@d.Tastiness</th>
                <th>@d.Calories</th>
            </tr>
        }
    </tbody>
</table>