@{
    ViewData["Title"] = "A Web Page";
    Layout = "_NoNav";
}
@model Survey

<div class="card w-50 form-control text-left m-3">
<h1>Submitted Info</h1>
<div>
        <p >Name: @Model.Name</p>
        <p>Dojo Location: @Model.Location</p>
        <p>Favorite Language: @Model.Language</p>
        <p>Comment (optional): @Model.Comment</p>
</div>
<a asp-action="Index" asp-controller="Home" class="btn btn-danger col-sm-2 item-align-end">Go Back</a>
</div>